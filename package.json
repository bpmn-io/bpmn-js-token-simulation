    "rollup-plugin-string": "^3.0.0",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.7.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "inherits-browser": "^0.1.0",
    "min-dash": "^4.0.0",
    "min-dom": "^4.0.3",
    "randomcolor": "^0.6.2"
